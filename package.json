{
  "name": "ticon-rn",
  "scheme": "TICON-RN",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --dev-client",
    "doctor": "npx expo-doctor",
    "doctor-install": "npx expo install --check",
    "ios-preview-build": "eas build -p ios --profile preview",
    "ios-preview-run": "eas build:run -p ios --profile preview",
    "pod": "cd ios && pod install && cd ..",
    "clean": "rm -rf node_modules && yarn install && rm -rf ios/Pods ios/Podfile.lock ios/build && yarn pod",
    "buildclean": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ios/build && pod cache clean --all",
    "prebuildclean": "expo prebuild --clean"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-firebase/app": "^18.6.0",
    "@react-native-firebase/auth": "^18.6.0",
    "@react-native-firebase/database": "^18.6.0",
    "@react-native-firebase/firestore": "^18.6.0",
    "@react-native-firebase/storage": "^18.6.0",
    "@react-native-google-signin/google-signin": "^10.1.0",
    "@react-native-picker/picker": "^2.5.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@react-navigation/stack": "^6.3.20",
    "@types/react": "~18.2.14",
    "expo": "^49.0.16",
    "expo-build-properties": "~0.8.3",
    "expo-dev-client": "~2.4.11",
    "expo-image-picker": "~14.3.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.5.0",
    "react": "18.2.0",
    "react-native": "^0.72.6",
    "react-native-recoil-persist": "^0.0.6",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-toast-message": "^2.1.7",
    "recoil": "^0.7.7",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "react-native-svg-transformer": "^1.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "private": true
}
